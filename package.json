{
  "name": "node-sass-js-importer",
  "version": "5.0.0",
  "description": "Allows importing CommonJS modules in Sass files parsed by node-sass.",
  "type": "module",
  "keywords": [
    "commonjs",
    "importer",
    "node-sass",
    "sass"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/loilo/node-sass-js-importer.git"
  },
  "license": "MIT",
  "files": [
    "src"
  ],
  "exports": {
    "import": "./src/importer.module.js",
    "require": "./src/importer.commonjs.cjs"
  },
  "main": "./src/importer.commonjs.cjs",
  "module": "./src/importer.module.js",
  "scripts": {
    "test": "vitest run"
  },
  "dependencies": {
    "execa": "^5.1.1",
    "is-plain-object": "^5.0.0"
  },
  "devDependencies": {
    "sass": "^1.69.5",
    "vitest": "^0.34.6"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}

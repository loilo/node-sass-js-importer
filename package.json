{
  "name": "node-sass-js-importer",
  "version": "6.0.0",
  "description": "Allows importing CommonJS modules in Sass files parsed by node-sass.",
  "type": "module",
  "keywords": [
    "commonjs",
    "importer",
    "node-sass",
    "sass"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/loilo/node-sass-js-importer.git"
  },
  "license": "MIT",
  "files": [
    "src"
  ],
  "exports": {
    "import": "./src/importer.mjs",
    "require": "./src/importer.cjs"
  },
  "main": "./src/importer.cjs",
  "module": "./src/importer.mjs",
  "scripts": {
    "test:types": "tsc",
    "test:vitest": "vitest run",
    "test": "run-s test:types test:vitest"
  },
  "devDependencies": {
    "@types/node": "^24.0.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.77.0",
    "typescript": "^5.5.4",
    "vitest": "^3.0.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
